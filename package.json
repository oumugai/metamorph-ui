{
  "name": "@oumugai/metamorph-ui",
  "version": "0.1.5",
  "description": "Modern UI framework for Solid.js with MetaMorph design pattern",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/metamorph-ui.umd.cjs",
  "module": "./dist/metamorph-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "style": "./dist/metamorph-ui.css",
        "default": "./dist/metamorph-ui.js"
      },
      "require": "./dist/metamorph-ui.umd.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "styles/*": ["./dist/styles/*"]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "peerDependencies": {
    "solid-js": "^1.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/node": "^20.17.24",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-preset-solid": "^1.9.5",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "components",
    "design-system",
    "metamorph"
  ],
  "author": "oumugai",
  "license": "MIT"
}
